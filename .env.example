# Google Analytics MEASUREMENT ID
NEXT_PUBLIC_GA_MEASUREMENT_ID=your-ga-id-here

# Google Adsense Client ID
NEXT_PUBLIC_GOOGLE_ADSENSE_ID=your-adsense-client-id

# NEXT_PUBLIC_USE_SHARED_DB (if u set 1 then u need not set MONGODB_URI) 
NEXT_PUBLIC_USE_SHARED_DB=0

# MongoDB URI
MONGODB_URI=your-mongodb-uri

# Home Listing 0 or 1
NEXT_PUBLIC_HOME_LISTING=1

# Private Submit 0 or 1
NEXT_PUBLIC_PRIVATE_SUBMIT=0

# Admin Password
ADMIN_PASSWORD=your-admin-password